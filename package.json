{
  "name": "api-express-cli",
  "version": "1.0.7-beta-6",
  "description": "api-express-cli is an tool with an integrated cli, which allows generating models, rutes, and controllers",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "bin": {
    "aec": "./build/lib/bin/aec.js",
    "api-express-cli": "./build/lib/bin/aec.js"
  },
  "scripts": {
    "build": "npx tsc",
    "watch": "npx tsc -w",
    "dev:run-server": "nodemon ./build/src/index",
    "prepare": "npm run build && npm run cp-file-txt",
    "cp-file-txt": "node ./build/lib/functions/files",
    "test": "node ./build/test/blah.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dev-package-npm/api-express-cli.git"
  },
  "keywords": [
    "api",
    "express",
    "cli",
    "api-express-cli",
    "ws",
    "websockets",
    "mvc",
    "mysql-promise",
    ".env"
  ],
  "author": "Jairo Rohatan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dev-package-npm/api-express-cli/issues"
  },
  "homepage": "https://github.com/dev-package-npm/api-express-cli#readme",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cryptr": "^4.0.1",
    "@types/express": "^4.17.17",
    "@types/inquirer": "^9.0.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.13.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "bcryptjs": "^2.4.3",
    "cryptr": "^6.1.0",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "inquirer": "^8.2.4",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "promise-mysql": "^5.2.0",
    "socket.io": "^4.6.0",
    "ts-code-generator": "^0.0.3"
  }
}