{
  "name": "api-express-cli",
  "version": "1.0.7-beta-9",
  "description": "api-express-cli is an tool with an integrated cli, which allows generating models, rutes, and controllers",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "bin": {
    "aec": "./build/index.js",
    "api-express-cli": "./build/index.js"
  },
  "scripts": {
    "build": "rm -rf build && npx tsc && npm run cp-file-txt",
    "watch-ts": "npm run build && npx tsc -w",
    "dev:build": "rm -rf dist && npx tsc -p ./tsconfig.dev.json",
    "dev:watch-ts": "npm run dev:build && npx tsc -p ./tsconfig.dev.json -w",
    "dev:run-server": "nodemon ./dist/index",
    "dev:watch-file": "node ./build/watch-file",
    "prepare": "npm run build",
    "cp-file-txt": "node ./build/functions/files",
    "test": "node ./build/test/blah.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dev-package-npm/api-express-cli.git"
  },
  "keywords": [
    "api",
    "express",
    "cli",
    "api-express-cli",
    "ws",
    "websockets",
    "mysql-promise",
    ".env",
    "cuby-orm",
    "cuby",
    "orm"
  ],
  "author": "Jairo Rohatan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dev-package-npm/api-express-cli/issues"
  },
  "homepage": "https://github.com/dev-package-npm/api-express-cli#readme",
  "devDependencies": {
    "@types/ansi-colors": "^3.2.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cryptr": "^4.0.1",
    "@types/express": "^4.17.17",
    "@types/inquirer": "^9.0.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.14.6",
    "@types/spinnies": "^0.5.0",
    "nodemon": "^2.0.21",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "bcryptjs": "^2.4.3",
    "cryptr": "^6.1.0",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "inquirer": "^8.2.4",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "spinnies": "^0.5.1",
    "ts-code-generator": "^0.0.3",
    "ts-mixer": "^6.0.3"
  }
}